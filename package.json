{
  "name": "se-2",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/hardhat",
      "packages/nextjs"
    ]
  },
  "scripts": {
    "account": "npm -w @se-2/hardhat run account",
    "chain": "npm -w @se-2/hardhat run chain",
    "fork": "npm -w @se-2/hardhat run fork",
    "deploy": "npm -w @se-2/hardhat run deploy",
    "verify": "npm -w @se-2/hardhat run verify",
    "hardhat-verify": "npm -w @se-2/hardhat run hardhat-verify",
    "compile": "npm -w @se-2/hardhat run compile",
    "generate": "npm -w @se-2/hardhat run generate",
    "flatten": "npm -w @se-2/hardhat run flatten",
    "hardhat:lint": "npm -w @se-2/hardhat run lint",
    "hardhat:lint-staged": "npm -w @se-2/hardhat run lint-staged",
    "hardhat:format": "npm -w @se-2/hardhat run format",
    "hardhat:test": "npm -w @se-2/hardhat run test",
    "test": "npm run hardhat:test",
    "format": "npm run next:format && npm run hardhat:format",
    "start": "npm -w @se-2/nextjs run dev",
    "next:lint": "npm -w @se-2/nextjs run lint",
    "next:format": "npm -w @se-2/nextjs run format",
    "next:check-types": "npm -w @se-2/nextjs run check-types",
    "next:build": "npm -w @se-2/nextjs run build",
    "next:serve": "npm -w @se-2/nextjs run serve",
    "postinstall": "husky install",
    "precommit": "lint-staged",
    "vercel": "npm -w @se-2/nextjs run vercel",
    "vercel:yolo": "npm -w @se-2/nextjs run vercel:yolo"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
