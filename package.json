{
  "name": "create-eth",
  "version": "1.0.3",
  "description": "Create a Scaffold-ETH-2 app",
  "repository": {
    "type": "git",
    "url": "https://github.com/scaffold-eth/create-eth.git"
  },
  "main": "dist/cli.js",
  "type": "module",
  "bin": "bin/create-dapp-se2.js",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:dev": "yarn build && rollup -c src/dev/rollup.config.js",
    "create-extension": "node dist/create-extension/create-extension.js",
    "dev": "rollup -c rollup.config.js --watch",
    "cli": "node bin/create-dapp-se2.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "changeset:release": "yarn build && changeset publish"
  },
  "keywords": [
    "cli",
    "ethereum",
    "scaffold-eth 2",
    "wagmi",
    "hardhat",
    "foundry",
    "viem",
    "rainbowkit"
  ],
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/ncp": "^2.0.8",
    "@types/node": "^20.19.8",
    "@types/semver": "^7.7.0",
    "@types/validate-npm-package-name": "^4.0.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "lefthook": "^1.12.2",
    "prettier": "^3.6.2",
    "rollup": "^4.45.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@changesets/cli": "^2.29.5",
    "@fastify/deepmerge": "^3.1.0",
    "@inquirer/prompts": "^7.6.0",
    "arg": "^5.0.2",
    "chalk": "^5.4.1",
    "execa": "^9.6.0",
    "listr2": "^9.0.1",
    "merge-packages": "^0.1.6",
    "ncp": "^2.0.0",
    "semver": "^7.7.2",
    "validate-npm-package-name": "^6.0.1"
  },
  "packageManager": "yarn@3.5.0"
}
