{
  "name": "@se-2/hardhat",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "account": "hardhat run scripts/listAccount.ts",
    "account:generate": "hardhat run scripts/generateAccount.ts",
    "account:import": "hardhat run scripts/importAccount.ts",
    "account:reveal-pk": "hardhat run scripts/revealPK.ts",
    "chain": "hardhat node",
    "check-types": "tsc --noEmit --incremental",
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "deploy": "hardhat ignition deploy ignition/modules/ScaffoldEthDeployModule.ts --network localhost",
    "flatten": "hardhat flatten",
    "fork": "MAINNET_FORKING_ENABLED=true hardhat node --network hardhat --no-deploy",
    "format": "prettier --write './**/*.(ts|sol)'",
    "generate": "yarn account:generate",
    "hardhat-verify": "hardhat verify",
    "lint": "eslint",
    "lint-staged": "eslint",
    "test": "REPORT_GAS=true hardhat test --network hardhat",
    "verify": "hardhat etherscan-verify"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ignition": "^3.0.3",
    "@nomicfoundation/hardhat-ignition-viem": "^3.0.3",
    "@nomicfoundation/hardhat-keystore": "^3.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^3.0.2",
    "@nomicfoundation/hardhat-node-test-runner": "^3.0.7",
    "@nomicfoundation/hardhat-toolbox-viem": "^5.0.1",
    "@nomicfoundation/hardhat-verify": "^3.0.4",
    "@nomicfoundation/hardhat-viem": "^3.0.0",
    "@nomicfoundation/hardhat-viem-assertions": "^3.0.3",
    "@typechain/hardhat": "^9.1.0",
    "@types/eslint": "^9.6.1",
    "@types/mocha": "^10.0.10",
    "@types/prettier": "^3.0.0",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "chai": "^4.5.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "hardhat": "^3.0.9",
    "prettier": "^3.5.3",
    "prettier-plugin-solidity": "^1.4.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.2",
    "typescript": "^5.8.2",
    "viem": "^2.38.5"
  },
  "dependencies": {
    "@inquirer/password": "^4.0.2",
    "@openzeppelin/contracts": "^5.0.2",
    "dotenv": "^16.4.5",
    "envfile": "^7.1.0",
    "qrcode": "^1.5.4"
  }
}
