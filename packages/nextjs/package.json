    "react": "^19.0.1",
    "react-dom": "^19.0.1",
    "react-hot-toast": "^2.4.0",
    "usehooks-ts": "^3.1.0",
    "viem": "2.39.0",
    "wagmi": "2.19.5",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
